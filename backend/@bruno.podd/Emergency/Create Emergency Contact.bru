meta {
  name: Create Emergency Contact
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/emergency-contacts
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
    "name": "John Doe",
    "phone": "+1234567890",
    "relationship": "spouse"
  }
}

vars:pre-request {
  baseUrl: http://127.0.0.1:8000
}
